    "mysql2": "3.15.1",
    "nodemailer": "7.0.6",
    "ollama": "0.6.0",
    "openai": "5.23.0",
    "pg": "8.16.3",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.7",
    "sqlite3": "5.1.7",
    "umzug": "3.8.2",
    "uuid": "13.0.0",
    "ws": "^8.18.3",
    "xlsx": "0.18.5",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/prompt": "20.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/graphql-upload": "17.0.0",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/luxon": "3.7.1",
    "@types/morgan": "1.9.10",
    "@types/node": "^24.5.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-node": "0.0.1-security",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-boundaries": "5.0.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "esm": "3.2.25",
    "jest": "^30.1.3",
    "jest-mock-extended": "4.0.0",
    "nodemon": "^3.1.10",
    "pkg": "5.8.1",
    "sequelize-cli": "6.6.3",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "babel-node": "babel-node --presets=@babel/preset-env",
    "build": "tsc --project .",
    "clean-install": "rm -rf node_modules && npm install",
    "commitlint": "commitlint -E HUSKY_GIT_PARAMS",
    "migration:run:public": "npm run babel-node src/api/lib/db/migrations/migrations.public.js up",
    "migration:tenants:run": "npm run babel-node src/api/lib/utils/migrate-tenants.js",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "start": "node build/server.js",
    "build:pkg": "pkg . --out-path dist",
    "build:all": "concurrently \"npm run build\" \"npm run build:pkg\"",
    "build:electron": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "rm": "rm -rf node_modules dist build package-lock.json",
    "dev": "nodemon --watch src --exec node -r ts-node/register -r tsconfig-paths/register ./src/server.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache",
    "migrate:up": "ts-node src/infrastructure/db/sequelize/migrations/cli.ts up"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "**/*",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "build/",
        "to": "resources/"
      }
    ]
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "public/**/*",
      "uploads/**/*",
      "config/**/*",
      ".env",
      "src/api/lib/types/**/*",
      "build/api/lib/types/**/*",
      "node_modules/sequelize/lib/**/*.js",
      "node_modules/sqlite3/**/*"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "**/*",
      "!**/*.map"
    ],
    "targets": [
      "node16-win-x64"
    ]
  }
}
