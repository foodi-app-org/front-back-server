    "i18next-fs-backend": "2.6.0",
    "i18next-http-middleware": "3.8.0",
    "iron-session": "8.0.4",
    "joi": "18.0.1",
    "jsonwebtoken": "9.0.2",
    "jwt-decode": "4.0.0",
    "luxon": "3.7.2",
