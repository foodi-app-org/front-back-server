{
  "name": "front-back-server",
  "version": "1.0.3",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "main.js",
  "bin": {
    "server": "build/app.js"
  },
  "dependencies": {
    "@babel/cli": "7.17.6",
    "@graphql-tools/load-files": "6.5.3",
    "@graphql-tools/merge": "8.2.2",
    "@graphql-tools/schema": "8.3.1",
    "apollo-server": "3.6.3",
    "apollo-server-express": "3.6.3",
    "bcrypt": "5.1.1",
    "body-parser": "1.19.2",
    "cookie-parser": "1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "express": "4.17.3",
    "googleapis": "100.0.0",
    "graphql": "16.3.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-upload": "13.0.0",
    "http": "0.0.1-security",
    "iron-session": "6.1.3",
    "joi": "^17.13.3",
    "js-base64": "3.7.2",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "3.1.2",
    "moment": "2.29.1",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "mysql2": "2.3.3",
    "nodemailer": "6.9.5",
    "pg": "8.11.3",
    "pg-hstore": "2.3.4",
    "sequelize": "^6.37.1",
    "sqlite3": "^5.1.7",
    "subscriptions-transport-ws": "0.11.0",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/node": "7.16.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/prompt": "^17.7.1",
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "babel-node": "0.0.1-security",
    "concurrently": "^9.1.2",
    "electron": "34.1.0",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "nodemon": "2.0.15",
    "pkg": "^5.8.1",
    "sequelize-cli": "^6.6.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitlint"
    }
  },
  "scripts": {
    "babel-node": "babel-node --presets=@babel/preset-env",
    "build": "babel src/ --out-dir build",
    "clean-install": "rm -rf node_modules && npm install",
    "commitlint": "commitlint -E HUSKY_GIT_PARAMS",
    "migration:run:public": "npm run babel-node src/api/lib/db/migrations/migrations.public.js up",
    "migration:tenants:run": "npm run babel-node src/api/lib/utils/migrate-tenants.js",
    "dev": "nodemon --exec npm run babel-node src/app.js",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint .",
    "migrate": "npx sequelize-cli db:migrate",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "start": "node src/app.js",
    "build:pkg": "pkg . --out-path dist",
    "build:all": "concurrently \"npm run build\" \"npm run build:pkg\"",
    "build:electron": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "**/*",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "build/",
        "to": "resources/"
      }
    ]
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "public/**/*",
      "uploads/**/*",
      "config/**/*",
      ".env"
    ],
    "targets": [
      "node18-win-x64"
    ]
  }
}
