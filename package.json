    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  },
  "scripts": {
    "babel-node": "babel-node --presets=@babel/preset-env",
    "build": "tsc --project .",
    "prepare-build": "npm run build && npm run postbuild:copy:gql",
    "postbuild:copy:gql": "cpy \"src/graphql/**/*.{gql,graphql}\" build/graphql --parents",
    "clean-install": "rm -rf node_modules && npm install",
    "commitlint": "commitlint -E HUSKY_GIT_PARAMS",
    "migration:run:public": "npm run babel-node src/api/lib/db/migrations/migrations.public.js up",
    "migration:tenants:run": "npm run babel-node src/api/lib/utils/migrate-tenants.js",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "start": "node -r tsconfig-paths/register build/server.js",
    "build:pkg": "pkg . --out-path dist",
    "build:all": "concurrently \"npm run build\" \"npm run build:pkg\"",
    "build:electron": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "rm": "rm -rf node_modules dist build package-lock.json",
    "dev": "nodemon --watch src --exec node -r ts-node/register -r tsconfig-paths/register ./src/server.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache",
    "migrate:up": "ts-node src/infrastructure/db/sequelize/migrations/cli.ts up",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "**/*",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "build/",
        "to": "resources/"
      }
    ]
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "public/**/*",
      "uploads/**/*",
      "config/**/*",
      ".env",
      "src/api/lib/types/**/*",
      "build/api/lib/types/**/*",
      "node_modules/sequelize/lib/**/*.js",
      "node_modules/sqlite3/**/*"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "**/*",
      "!**/*.map"
    ],
    "targets": [
      "node16-win-x64"
    ]
  }
}
